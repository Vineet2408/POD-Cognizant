package com.cts.portal.service;


import com.cts.portal.model.PatientDetail;
import com.cts.portal.model.TreatmentPlan;

import retrofit2.Call;
import retrofit2.http.GET;
import retrofit2.http.Path;

public interface IPTMSPatientInterface 
{
	@GET("/getPatient/{ptId}")
	public Call<PatientDetail> getPatient(@Path("ptID") long ptId);
	
	@GET("/getplan/{ptId}")
	public Call<TreatmentPlan> getTreatmentPlan(@Path("ptID") int ptId );

}
